<environment include="Development">
    <!-- NOTE: Enable the bundle for faster loading and optimized caching! -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css" integrity="sha384-Zug+QiDoJOrZ5t4lssLdxGhVrurbmBWopoEl+M6BdEfwnCJZtKxi1KgxUyJq13dy" crossorigin="anonymous">
    <!-- For use databels in b4 -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap4.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.5.1/css/buttons.dataTables.min.css" />
    
    <!-- Jquery UI -->
    <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/themes/ui-lightness/jquery-ui.css" />
    <link href="~/css/bootstrap-4-navbar.css" rel="stylesheet" />

    <!-- Font Awesome is CDN now, and use last version -->
    <script defer src="https://use.fontawesome.com/releases/v5.0.2/js/all.js"></script>
    <!-- SmartAdmin Styles : Caution! DO NOT change the order -->
    <link rel="stylesheet" href="~/css/smart.css" />
    <!-- We recommend you use "site.css" to override. -->
    <link rel="stylesheet" href="~/css/site.css" />
</environment>
<environment exclude="Development">
    <!-- NOTE: Enable the bundle for faster loading and optimized caching! -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css" integrity="sha384-Zug+QiDoJOrZ5t4lssLdxGhVrurbmBWopoEl+M6BdEfwnCJZtKxi1KgxUyJq13dy" crossorigin="anonymous">
    <!-- For use databels in b4 -->
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap4.min.css" />
    <!-- Jquery UI -->
    <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/jquery.ui/1.12.1/themes/ui-lightness/jquery-ui.css" />
    <!-- Font Awesome is CDN now, and use last version -->
    <script defer src="https://use.fontawesome.com/releases/v5.0.2/js/all.js"></script>
    <!-- SmartAdmin Styles : Caution! DO NOT change the order -->
    <link rel="stylesheet" href="~/css/smart.min.css" asp-append-version="true" />
    <!-- We recommend you use "site.css" to override. -->
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
</environment>
